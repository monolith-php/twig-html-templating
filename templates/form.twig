{% macro open() %}
{{ form_model.open | raw }}
{% endmacro %}

{% macro field(name, attributes) %}
{{ attribute(form_model, name, [attributes]) | raw }}
{% endmacro %}

{% macro submit(button_text) %}
{{ form_model.submit(button_text) | raw }}
{% endmacro %}

{% macro close() %}
{{ form_model.close | raw }}
{% endmacro %}